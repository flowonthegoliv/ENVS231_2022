        "a = 2 #change the input value\n",
        "if a == 1: #if a is equal to 1 \n",
        "  print('Hola')\n",
        "if a == 2: #if a is equal to 1 \n",
        "  print('AdiÃ³s')\n",
        "if a > 2: #if a is greater than 2 \n",
        "  print('Jorge')"
      ],
